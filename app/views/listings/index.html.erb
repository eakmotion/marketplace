<div class="jumbotron">
  <h1>A MARKETPLACE CURATED FOR YOU</h1>
  <h2>Discover one-of-a-kind items</h2>
</div>

<div class="center">
  <div class="row">
    <% @listings.each do |listing| %>
      <div class="col-md-3">
        <div class="thumbnail">
          <div class="caption">
            <h3><%= listing.name %></h3>
            <p><%= number_to_currency(listing.price) %></p>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
